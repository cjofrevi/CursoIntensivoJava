package beca;

public class TareaArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(int i: arregloCien()) {
			System.out.println(i);
		}

	}
	
	public static int[] arregloCien() {
		int[] array = new int[100];
		for(  int i= 0; i<100; i++) {
			array[i] = i+1;
		}
		
		return array;
	}

}
